type t_data = int # Par ex

fsm model Emetteur(
     in h: event,
     in r_rdy: bool,
     out data: t_data,
     out e_rdy: bool)
  {
  states: E1, E2;
  vars:
    cnt: int;
  trans:
    E1 -- h.r_rdy=1 | data:=cnt; cnt:=cnt+1; e_rdy:=1 -> E2,
    E2 -- h.r_rdy=0 | e_rdy:=0 -> E1;
  itrans: | e_rdy:=0; cnt:=1 -> E1;
  }

