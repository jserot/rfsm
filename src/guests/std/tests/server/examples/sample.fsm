fsm model gensig(
  in h: event, 
  in e: bool,
  out s: bool)
  {
  states: E0 where s=0, E1 where s=1, E2 where s=1, E3 where s=1;
  trans:
  | E0 -> E1 on h when e=1
  | E1 -> E2 on h
  | E2 -> E3 on h
  | E3 -> E0 on h;
  itrans:
  | -> E0;
  }

input H : event = periodic (10,10,80)
input e : bool = value_changes (0:0, 25:1, 35:0)
output s1, s2, s3 : bool 

fsm g = gensig (H,e,s1)


