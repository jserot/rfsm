-- This example illustrates the use of user-defined enum types

type switch = enum { On, Off }

fsm model foo(
  in h: event,
  in e: switch)
{
  states: E0, E1;
  trans:
  | E0 -> E1 on h when e=On
  | E1 -> E0 on h when e=Off;
  itrans:
  | -> E0 ;
}

input h : event = periodic(10, 10, 30)
input inp : switch = value_changes(5:On, 15:Off)

fsm f = foo(h,inp)
